{
  "name": "@dreamystify/fingrprint",
  "version": "0.1.28",
  "description": "A distributed, k-sortable unique ID generation system using Redis and Lua, inspired by Icicle.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run clean && tsc -p . && node .scripts/copy-assets.js",
    "watch": "tsc -w -p .",
    "clean": "rm -rf lib",
    "test": "jest --runInBand --detectOpenHandles",
    "coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dreamystify/fingrprint.git"
  },
  "keywords": [
    "k-sortable",
    "id",
    "generator",
    "bigint"
  ],
  "author": "Dreamystify",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dreamystify/fingrprint/issues"
  },
  "homepage": "https://github.com/Dreamystify/fingrprint#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "jest": "^29.7.0",
    "nyc": "^17.1.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "big-integer": "^1.6.51",
    "ioredis": "^5.4.2",
    "redis": "^4.6.7"
  }
}
