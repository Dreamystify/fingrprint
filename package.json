{
  "name": "@dreamystify/fingrprint",
  "version": "0.1.5",
  "description": "A distributed, k-sortable unique ID generation system using Redis and Lua, inspired by Icicle.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "rm -rf lib",
    "test": "npm run build && mocha -r ts-node/register/transpile-only -r source-map-support/register  --recursive tests/**/*.spec.ts --timeout 50000",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dreamystify/fingrprint.git"
  },
  "keywords": [
    "k-sortable",
    "id",
    "generator",
    "bigint"
  ],
  "author": "Dreamystify",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dreamystify/fingrprint/issues"
  },
  "homepage": "https://github.com/Dreamystify/fingrprint#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "big-integer": "^1.6.51",
    "redis": "^4.1.0"
  }
}
